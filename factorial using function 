#include <stdio.h> 
int fact(int x, int y){ 
    int i; 
    for(i=1;i<=x;i++){
        y=y*i;
    }
    return y;
}
int main(){
    int n, sum=1,res; 
    printf("Enter the factorial number required:");
    scanf("%d",&n); 
    res=fact(n,sum);
    printf("\nFactorial of %d = %d",n,res);
    return 0;
}